{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "comment-format": [
      true,
      "check-space"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "trailing-comma": [
      false
    ]
  },
  "rulesDirectory": []
}
